{
  "name": "tm-portal-react-new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "shell",
    "shared",
    "sales-mfe",
    "finance-mfe",
    "reports-mfe",
    "access-control-mfe",
    "brands-mfe",
    "documents-mfe",
    "config-mfe"
  ],
  "scripts": {
    "dev:all": "concurrently \"npm run dev --workspace=shell\" \"npm run dev:remote --workspace=sales-mfe\" \"npm run dev:remote --workspace=finance-mfe\" \"npm run dev:remote --workspace=reports-mfe\" \"npm run dev:remote --workspace=access-control-mfe\" \"npm run dev:remote --workspace=brands-mfe\" \"npm run dev:remote --workspace=documents-mfe\" \"npm run dev:remote --workspace=config-mfe\" \"npm:tailwind\"",
    "install:all": "npm install",
    "build:all": "npm run build --workspaces --if-present",
    "build:remotes": "npm run build --workspace=sales-mfe && npm run build --workspace=finance-mfe && npm run build --workspace=reports-mfe && npm run build --workspace=access-control-mfe && npm run build --workspace=brands-mfe && npm run build --workspace=documents-mfe && npm run build --workspace=config-mfe",
    "kill:ports": "taskkill /f /im node.exe /t || true",
    "tailwind": "npx @tailwindcss/cli -i ./shared/src/styles/tailwind.css -o ./shared/src/styles/main.css --watch",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^41.4.2",
    "@ckeditor/ckeditor5-react": "^11.0.0",
    "@headlessui/react": "^2.2.7",
    "@hello-pangea/dnd": "^18.0.1",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@reduxjs/toolkit": "^2.9.0",
    "@tailwindcss/cli": "^4.1.12",
    "axios": "^1.11.0",
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "react": "^19.1.1",
    "react-data-table-component": "^7.7.0",
    "react-date-range": "^2.0.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "react-select": "^5.10.2",
    "recharts": "^3.4.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "vite": "^7.1.2"
  }
}
